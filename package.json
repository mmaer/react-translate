{
  "name": "react-translates",
  "version": "1.0.4",
  "description": "Translate component that allows translate translations into other languages.",
  "author": "Kamil Kruczek",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmaer/react-translates.git"
  },
  "scripts": {
    "clean": "rm -rf dist browser",
    "compile": "BABEL_ENV=production babel src --out-dir dist --copy-files --source-maps --ignore \"src/test/**\"",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "start": "BABEL_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "pretest": "run-p clean lint",
    "test": "BABEL_ENV=test nyc npm run mocha",
    "mocha": "BABEL_ENV=test mocha --require ./src/test/index.js './src/test/**/*.js'"
  },
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-rewire": "^1.0.0-rc-6",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-statics": "0.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babel-template": "^6.14.0",
    "babel-traverse": "^6.14.0",
    "babel-types": "^6.14.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "mocha-sinon": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^12.0.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.2",
    "react-test-renderer": "^16.4.1",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.12.2"
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/main/**/*.js"
    ],
    "exclude": [
      "flow-typed/**"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "sourceMap": true,
    "cache": false,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
